@model IEnumerable<efcorePro.Models.CountryModel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Displayvilage</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>
    <h5>
        @ViewBag.Message
    </h5>
    <table class="table">
        <thead>
            <tr>
                <th>Village Id</th>
                <th>Village Name</th>
                <th>Village District</th>
                <th>Village PinCode</th>
                <th>View</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <td> @item.Vid </td>
                    <td> @item.Vname </td>
                    <td> @item.Vdist </td>
                    <td> @item.PinCode </td>
                    <td> @Html.ActionLink("View", "View", "LinqandspEf", new{Vid = item.Vid}, null) </td>
                    <td> @Html.ActionLink("Edit", "Edit", "LinqandspEf", new{Vid = item.Vid}, null) </td>
                    <td> @Html.ActionLink("Delete", "Delete", "LinqandspEf", new{Vid=item.Vid}, new{onClick="return confirm('Are you realy want to delete....?')"}) </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
